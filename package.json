{
  "name": "angular-leadgen",
  "productName": "angular-leadgen",
  "version": "1.9.1",
  "description": "The Spinifex foundation code for Electron based Kiosk applications",
  "license": "UNLICENSED",
  "repository": "git@bitbucket.org:spinifexgrouplax/angular-leadgen.git",
  "main": "dist-module/main.js",
  "author": {
    "name": "Jeremy Bunting",
    "email": "jeremy.bunting@spinifexgroup.com",
    "url": "http://spinifexgroup.com"
  },
  "scripts": {
    "test": "electron-tap  --fixture 'test/index.html' --renderer 'test/*.js'",
    "start": "gulp watch",
    "logout": "node helpers.js --action=logout --file=./build/leadgen_config.json",
    "set_wheelstand": "node helpers.js --action=wheelstand --file=./build/leadgen_config.json",
    "set_kiosk": "node helpers.js --action=kiosk --file=./build/leadgen_config.json",
    "build": "export NODE_ENV=production && electron-packager ./build --out=dist --app-version=$npm_package_version --prune --overwrite --arch=ia32 --arch=x64 --platform=darwin --platform=win32",
    "scss": "node-sass --output-style compressed -o ./css ./sass",
    "update": "npm install && bower install && gulp dist:module",
    "watch:css": "onchange 'sass/*.scss' -- npm run scss",
    "changelog": "conventional-changelog -p jscs -i CHANGELOG.md -s -r 0"
  },
  "files": [
    "dist-module/",
    "main-process.js",
    "helpers.js"
  ],
  "keywords": [
    "electron-app",
    "electron"
  ],
  "dependencies": {
    "angular": "1.5.8",
    "angular-PubSub": "^1.0.0",
    "angular-bind-html-compile": "https://github.com/incuna/angular-bind-html-compile",
    "angular-bootstrap-calendar": "^0.28.1",
    "angular-chosen-localytics": "^1.5.1",
    "angular-sanitize": "1.5.8",
    "angular-ui-bootstrap": "^2.3.0",
    "auto-launch": "^5.0.1",
    "bootstrap-css-only": "^3.3.7",
    "chosen": "https://github.com/milkmanjr/chosen.git#v1.6.7",
    "contentful": "^3.3.14",
    "electron-debug": "^1.0.1",
    "electron-localshortcut": "^0.6.1",
    "is-online": "^5.1.2",
    "jquery": "^3.0.0",
    "js-base64": "^2.1.9",
    "moment": "^2.17.1",
    "nedb": "^1.8.0",
    "ng-table": "^3.0.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "sha1": "^1.1.1",
    "uuid": "^2.0.2",
    "yargs": "^7.1.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.3",
    "babel-core": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "bower": "^1.7.9",
    "browser-sync": "^2.13.0",
    "del": "^2.2.1",
    "devtron": "^1.2.0",
    "electron-packager": "^7.0.0",
    "electron-prebuilt": "^1.2.0",
    "electron-rebuild": "^1.5.7",
    "electron-tap": "https://github.com/milkmanjr/electron-tap.git#v0.1.8",
    "gulp": "^3.9.1",
    "gulp-angular-embed-templates": "^2.2.0",
    "gulp-babel": "^6.1.2",
    "gulp-changed": "^1.3.0",
    "gulp-concat": "^2.6.0",
    "gulp-exec": "^2.1.2",
    "gulp-if": "^2.0.1",
    "gulp-load-plugins": "^1.2.4",
    "gulp-main-bower-files": "^1.5.1",
    "gulp-nginclude": "^0.4.8",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.1",
    "gulp-task-listing": "^1.0.1",
    "gulp-useref": "^3.1.0",
    "gulp-xo": "^0.11.0",
    "jasmine": "^2.5.2",
    "jasmine-core": "^2.5.2",
    "jasmine-node": "^1.14.5",
    "jasmine-spec-reporter": "^2.7.0",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^2.0.0",
    "karma-electron-launcher": "^0.1.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.4",
    "karma-spec-reporter": "0.0.26",
    "node-sass": "^3.7.0",
    "npm-main-files": "^1.0.0",
    "onchange": "^2.5.0",
    "spectron": "^3.4.0",
    "tape": "^4.6.3",
    "xo": "^0.15.0",
    "xtend": "^4.0.1"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ],
    "ignores": [
      "bin/**",
      "build/**",
      "tests/**",
      "gulpfile.babel.js",
      "karma.conf.js"
    ]
  }
}
